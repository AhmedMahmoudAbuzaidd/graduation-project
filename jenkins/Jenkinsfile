pipeline {
    agent { label 'master' }

    stages {
        stage('SCM') {
            steps {
                git branch: 'main', url: 'https://github.com/AhmedMahmoudAbuzaidd/graduation-project.git'
            }
        }
        stage('SonarQube Analysis') {
            environment {
                SONAR_SCANNER_HOME = tool name: 'SonarScannerForMSBuild', type: 'SonarScannerTool'
                PATH = "${env.PATH}:${env.HOME}/.dotnet/tools"
            }
            steps {
                withSonarQubeEnv('sonarqube') {
                    sh 'dotnet-sonarscanner begin /k:"scan-dotnet"'
                    // Build your project here
                    // sh 'dotnet build <your-project>'
                    sh 'dotnet-sonarscanner end'
                }
            }
        }
    }
}
